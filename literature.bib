@article{choidatta.jes.2005.protondiffusion,
author = {Choi, Pyoungho and Jalani, Nikhil H. and Datta, Ravindra}, 
title = {Thermodynamics and Proton Transport in Nafion: II. Proton Diffusion Mechanisms and Conductivity},
volume = {152}, 
number = {3}, 
pages = {E123-E130}, 
year = {2005}, 
doi = {10.1149/1.1859814}, 
abstract ={A comprehensive pore transport model is proposed to describe proton diffusion within Nafion at various hydration levels by incorporating effects of water uptake and various proton transport mechanisms, namely, proton hopping along surface, Grotthuss diffusion, and ordinary mass diffusion of hydronium ions. The diffusion coefficients are predicted within a general random walk framework. The proton conductivity in contact with water vapor is accurately predicted as a function of relative humidity without any fitted parameters, considering the sorption isotherm proposed in the companion paper (Part I). A maximum conductivity in contact with liquid water is also predicted by the model for equivalent weight between 900 and 1000, in good agreement with the experimental measurements. The modeling framework could be extended to other proton conducting electrolytes for fuel cell applications. Â© 2005 The Electrochemical Society. All rights reserved.}, 
URL = {http://jes.ecsdl.org/content/152/3/E123.abstract}, 
eprint = {http://jes.ecsdl.org/content/152/3/E123.full.pdf+html}, 
journal = {J. Electrochem. Soc.} 
}

